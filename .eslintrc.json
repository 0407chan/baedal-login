{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb", 
        "airbnb/hooks",
        "airbnb-typescript"
    ],
    "overrides": [
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "@typescript-eslint"
    ],
    "rules": {
        "react/react-in-jsx-scope": "off"
    }
}
